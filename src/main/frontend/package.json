{
  "name": "repartir",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json",
    "build": "npm run check-compile && npm run build --workspaces && npm run build:app",
    "build:app": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test-ci": "ng test --karma-config=karma-ci.conf.js",
    "acceptance-test": "npx bddgen --config ../../jsAcceptanceTest/src/playwright.config.ts && npx playwright test --config ../../jsAcceptanceTest/src/playwright.config.ts",
    "acceptance-test:e2e": "npx bddgen --config ../../jsAdvancedAcceptanceTest/src/playwright.config.e2e.ts && cross-env ASSEMBLY_NAME=e2e npx playwright test --config ../../jsAdvancedAcceptanceTest/src/playwright.config.e2e.ts --grep-invert @ignore-assembly-e2e",
    "acceptance-test:mock-api": "npx bddgen --config ../../jsAdvancedAcceptanceTest/src/playwright.config.mockApi.ts && cross-env ASSEMBLY_NAME=mock-api npx playwright test --config ../../jsAdvancedAcceptanceTest/src/playwright.config.mockApi.ts --grep-invert @ignore-assembly-mock-api",
    "acceptance-test:backend": "npx bddgen --config ../../jsAdvancedAcceptanceTest/src/playwright.config.http.ts && cross-env ASSEMBLY_NAME=backend npx playwright test --config ../../jsAdvancedAcceptanceTest/src/playwright.config.http.ts --grep-invert @ignore-assembly-backend",
    "wiremock": "wiremock",
    "check-compile": "npx tsc --noEmit --skipLibCheck",
    "cy-acceptance-test:e2e": "cypress run --e2e --headed --env ASSEMBLY_NAME=e2e,TAGS='not @ignore-assembly-e2e'",
    "cy-acceptance-test:backend": "cypress run --e2e --headed --env ASSEMBLY_NAME=backend,TAGS='not @ignore-assembly-backend'",
    "acceptance-test-cy:e2e": "start-server-and-test 'npm run start-servers' \"http://localhost:8080|http://localhost:8081|http://localhost:4200\" cy-acceptance-test:e2e",
    "acceptance-test-cy:backend": "start-server-and-test 'npm run start-servers' \"http://localhost:8080|http://localhost:8081|http://localhost:4200\" cy-acceptance-test:backend",
    "start-servers": "concurrently \"npm run start:api\" \"npm run start\" \"npm run wiremock -- --port 8081\"",
    "start:api": "node ../../../iniciar-backend.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.12",
    "@angular/cdk": "^15.2.9",
    "@angular/common": "^16.2.12",
    "@angular/compiler": "^16.2.12",
    "@angular/core": "16.2.12",
    "@angular/forms": "^16.2.12",
    "@angular/platform-browser": "^16.2.12",
    "@angular/platform-browser-dynamic": "^16.2.12",
    "@angular/router": "^16.2.12",
    "@badeball/cypress-cucumber-preprocessor": "^22.0.0",
    "primeflex": "^3.0.1",
    "primeicons": "^6.0.1",
    "primeng": "^16.9.1",
    "rxjs": "~6.6.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.16",
    "@angular-devkit/core": "^16.2.16",
    "@angular/cli": "^16.2.16",
    "@angular/compiler-cli": "^16.2.12",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.2.4",
    "@ngtools/webpack": "^16.2.16",
    "@playwright/test": "1.48.1",
    "@types/jasmine": "~3.8.0",
    "@types/node": "^12.11.1",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "cypress": "^14.0.2",
    "jasmine-core": "~3.8.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "karma-junit-reporter": "^2.0.1",
    "monocart-coverage-reports": "^2.11.1",
    "monocart-reporter": "^2.9.6",
    "playwright": "1.48.1",
    "playwright-bdd": "^7.5.0",
    "start-server-and-test": "^2.0.10",
    "ts-node": "^10.0.0",
    "typescript": "~4.9.3",
    "wiremock": "^3.9.1"
  },
  "workspaces": [
    "packages/assembly-runner"
  ]
}
